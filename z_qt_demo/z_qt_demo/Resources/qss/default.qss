/**************************** QCheckBox ****************************/
QCheckBox {
    color:white;
    font-family: "Microsoft YaHei";
    font-size: 14px;
}

QCheckBox::indicator {     
     width: 14px;
     height:14px;     
}

QCheckBox::indicator:!checked {
    border-image:url(:/checkbox/checkbox_normal) 0 0 0 0 stretch stretch;
}

QCheckBox::indicator:hover:!checked {
    border-image:url(:/checkbox/checkbox_hover) 0 0 0 0 stretch stretch;
}

QCheckBox::indicator:checked {
    border-image:url(:/checkbox/checkbox_selected_normal) 0 0 0 0 stretch stretch;
}

QCheckBox::indicator:checked:hover {
    border-image:url(:/checkbox/checkbox_selected_hover) 0 0 0 0 stretch stretch;
}

QCheckBox[group="CheckBoxGroup2"] {
    color: white;
    font-family: "Microsoft YaHei";
    font-size: 14px;
    background-color:#012D5B;
}

QCheckBox::indicator[group="CheckBoxGroup2"] {     
	width: 0px;
	height: 0px;  
}

QCheckBox::checked[group="CheckBoxGroup2"] {
    color: white;
    font-family: "Microsoft YaHei";
    font-size: 14px;
    background-color:#003B90;
}

QCheckBox::hover[group="CheckBoxGroup2"] {
    color: white;
    font-family: "Microsoft YaHei";
    font-size: 14px;
    background-color:#11ABFF;
}
