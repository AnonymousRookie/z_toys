/**************************** QScrollBar ****************************/
QScrollBar:vertical {
    width:8px;
    background:rgba(30,144,255,0%);
    margin:0px,0px,0px,0px;
    padding-top:9px;
    padding-bottom:9px;
}

QScrollBar::handle:vertical {
    width:8px;
    background:rgba(30,144,255,25%);
    border-radius:4px;
    min-height:20;
}

QScrollBar::handle:vertical:hover {
    width:8px;
    background:rgba(30,144,255,50%);
    border-radius:4px;
    min-height:20;
}

QScrollBar::add-line:vertical {
    height:9px;width:8px;
    border-image:url(:/scrollbar/3);
    subcontrol-position:bottom;
}

QScrollBar::sub-line:vertical {
    height:9px;width:8px;
    border-image:url(:/scrollbar/1);
    subcontrol-position:top;
}

QScrollBar::add-line:vertical:hover {
    height:9px;width:8px;
    border-image:url(:/scrollbar/4);
    subcontrol-position:bottom;
}

QScrollBar::sub-line:vertical:hover {
    height:9px;width:8px;
    border-image:url(:/scrollbar/2);
    subcontrol-position:top;
}

QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical {
    background:rgba(30,144,255,10%);
    border-radius:4px;
}

QScrollBar:horizontal {
    height:8px;
    background:rgba(30,144,255,0%);
    margin:0px,0px,0px,0px;
    padding-left:9px;
    padding-right:9px;
}

QScrollBar::handle:horizontal {
    height:8px;
    background:rgba(30,144,255,25%);
    border-radius:4px;
    min-height:20;
}

QScrollBar::handle:horizontal:hover {
    height:8px;
    background:rgba(30,144,255,50%);
    border-radius:4px;
    min-height:20;
}

QScrollBar::add-line:horizontal {
    width:9px;height:8px;
    border-image:url(:/scrollbar/7);
    subcontrol-position:right;
}

QScrollBar::sub-line:horizontal {
    width:9px;height:8px;
    border-image:url(:/scrollbar/5);
    subcontrol-position:left;
}

QScrollBar::add-line:horizontal:hover {
    width:9px;height:8px;
    border-image:url(:/scrollbar/8);
    subcontrol-position:right;
}

QScrollBar::sub-line:horizontal:hover {
    width:9px;height:8px;
    border-image:url(:/scrollbar/6);
    subcontrol-position:left;
}

QScrollBar::add-page:horizontal,QScrollBar::sub-page:horizontal {
    background:rgba(30,144,255,10%);
    border-radius:4px;
}

/**************************** QMenu ****************************/
QMenu {
    background-color: #0F344E;
    border-width:0px;
    margin: 2px;
}

QMenu::item {
    background-color: #0F344E;
    padding: 2px 25px 2px 20px;
    border-color:transparent;
    color:#B7C0C2;
}

QMenu::item:selected {
    border-width:1px;
    border-color:gray;
    background: #1B2D45;
    color: white;
}
QMenu::separator {
    height: 1px;
    background: #1D99A9;
}